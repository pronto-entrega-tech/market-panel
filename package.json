{
  "name": "market-panel",
  "version": "1.0.0",
  "description": "Veja seus pedidos e atualize o estoque.",
  "author": "ProntoEntrega",
  "main": "electron/main.js",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack serve --config ./webpack.development.js > scripts/webpack-dev-server.log 2> scripts/webpack-dev-server-error.log",
    "desktop:dev": "concurrently --success first \"node scripts/prepareDevServer.js\" \"node scripts/launchDevServer.js\" -k",
    "prod": "yarn prod-build && electron .",
    "build": "yarn prod-build && electron-builder --windows",
    "prod-build": "cross-env NODE_ENV=production npx webpack --config ./webpack.production.js",
    "audit-app": "npx electronegativity -i ./ -x LimitNavigationGlobalCheck,PermissionRequestHandlerGlobalCheck",
    "format": "prettier . --write",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "type-check": "tsc --noEmit",
    "test": "mocha",
    "postinstall": "electron-builder install-app-deps",
    "fix": "rm yarn.lock && npm i && yarn import && yarn && rm package-lock.json"
  },
  "build": {
    "publish": [
      {
        "provider": "generic",
        "url": "https://static.prontoentrega.com.br/download/"
      }
    ],
    "appId": "com.prontoentrega.prontoentrega",
    "productName": "Pronto Entrega",
    "artifactName": "Pronto Entrega Instalador.${ext}",
    "directories": {
      "buildResources": "resources"
    },
    "files": [
      "build/**/*",
      "electron/**/*"
    ],
    "win": {
      "target": [
        "nsis"
      ]
    },
    "nsis": {
      "installerLanguages": "pt_BR",
      "language": 1046
    },
    "copyright": "Copyright Â© 2022 ${author}"
  },
  "browserslist": [
    "last 2 Chrome versions"
  ],
  "dependencies": {
    "@loadable/component": "^5.15.2",
    "@mui/material": "^5.9.0",
    "@mui/styled-engine": "npm:@mui/styled-engine-sc@latest",
    "axios": "^0.27.2",
    "date-fns": "^2.29.1",
    "electron-updater": "^5.0.5",
    "glob": "^8.0.3",
    "immer": "^9.0.15",
    "keytar": "^7.9.0",
    "lodash": "4.17.21",
    "mdi-material-ui": "^7.4.0",
    "node-hid": "^2.1.1",
    "pg": "^8.7.3",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-qr-code": "^2.0.7",
    "react-router-dom": "^6.3.0",
    "react-window": "^1.8.6",
    "reflect-metadata": "^0.1.13",
    "scheduler": "^0.23.0",
    "secure-electron-context-menu": "^1.3.3",
    "secure-electron-store": "^4.0.2",
    "socket.io-client": "^4.5.1",
    "styled-components": "^5.3.5",
    "use-context-selector": "^1.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@doyensec/electronegativity": "^1.9.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@svgr/webpack": "^6.2.1",
    "@types/glob": "^7.2.0",
    "@types/loadable__component": "^5.13.4",
    "@types/node-hid": "^1.3.1",
    "@types/pg": "^8.6.5",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-window": "^1.8.5",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "babel-loader": "^8.2.5",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-styled-components": "^2.0.7",
    "babel-plugin-transform-inline-environment-variables": "^0.4.4",
    "buffer": "^6.0.3",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^7.2.2",
    "cross-env": "^7.0.3",
    "crypto-browserify": "^3.12.0",
    "csp-html-webpack-plugin": "^5.1.0",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "electron": "^19.0.6",
    "electron-builder": "^23.1.0",
    "electron-debug": "^3.2.0",
    "electron-devtools-installer": "git+https://github.com/ganeshrvel/electron-devtools-installer.git#fix/electron-18",
    "eslint": "^8.20.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-loader": "^3.1.2",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.1",
    "mocha": "^10.0.0",
    "path-browserify": "^1.0.1",
    "prettier": "^3.3.3",
    "react-refresh": "^0.14.0",
    "spectron": "^19.0.0",
    "stream-browserify": "^3.0.0",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3",
    "webpack-merge": "^5.8.0"
  },
  "resolutions": {
    "@mui/styled-engine": "npm:@mui/styled-engine-sc@latest"
  }
}
