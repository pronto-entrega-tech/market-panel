{
  "name": "market-panel",
  "version": "1.0.0",
  "description": "Veja seus pedidos e atualize o estoque.",
  "author": "ProntoEntrega",
  "main": "electron/main.cjs",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite",
    "desktop:dev": "cross-env NODE_ENV=development concurrently --success first -k \"vite\" \"electron .\"",
    "build": "vite build && electron-builder --windows",
    "audit-app": "npx electronegativity -i ./ -x LimitNavigationGlobalCheck,PermissionRequestHandlerGlobalCheck",
    "format": "prettier . --write",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "type-check": "tsc --noEmit",
    "test": "mocha",
    "postinstall": "electron-builder install-app-deps",
    "fix": "rm yarn.lock && npm i && yarn import && yarn && rm package-lock.json"
  },
  "build": {
    "publish": [
      {
        "provider": "generic",
        "url": "https://static.prontoentrega.com.br/download/"
      }
    ],
    "appId": "com.prontoentrega.prontoentrega",
    "productName": "Pronto Entrega",
    "artifactName": "Pronto Entrega Instalador.${ext}",
    "directories": {
      "buildResources": "resources"
    },
    "files": [
      "dir/**/*",
      "electron/**/*"
    ],
    "win": {
      "target": [
        "nsis"
      ]
    },
    "nsis": {
      "installerLanguages": "pt_BR",
      "language": 1046
    },
    "copyright": "Copyright Â© 2022 ${author}"
  },
  "browserslist": [
    "last 2 Chrome versions"
  ],
  "dependencies": {
    "@loadable/component": "^5.15.2",
    "@mui/material": "^5.9.0",
    "@mui/styled-engine": "npm:@mui/styled-engine-sc@latest",
    "axios": "^0.27.2",
    "date-fns": "^2.29.1",
    "electron-updater": "^5.0.5",
    "glob": "^8.0.3",
    "immer": "^9.0.15",
    "keytar": "^7.9.0",
    "lodash": "4.17.21",
    "mdi-material-ui": "^7.4.0",
    "node-hid": "^2.1.1",
    "pg": "^8.7.3",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-qr-code": "^2.0.7",
    "react-router-dom": "^6.3.0",
    "react-window": "^1.8.6",
    "reflect-metadata": "^0.1.13",
    "scheduler": "^0.23.0",
    "secure-electron-context-menu": "^1.3.3",
    "secure-electron-store": "^4.0.2",
    "socket.io-client": "^4.5.1",
    "styled-components": "^5.3.5",
    "ts-pattern": "^5.3.1",
    "use-context-selector": "^1.4.1"
  },
  "devDependencies": {
    "@doyensec/electronegativity": "^1.9.1",
    "@eslint/js": "^9.11.0",
    "@types/glob": "^7.2.0",
    "@types/loadable__component": "^5.13.4",
    "@types/node-hid": "^1.3.1",
    "@types/pg": "^8.6.5",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-window": "^1.8.5",
    "@types/styled-components": "^5.1.25",
    "@vitejs/plugin-react": "^4.3.1",
    "concurrently": "^7.2.2",
    "cross-env": "^7.0.3",
    "electron": "^19.0.6",
    "electron-builder": "^23.1.0",
    "electron-debug": "^3.2.0",
    "electron-devtools-installer": "git+https://github.com/ganeshrvel/electron-devtools-installer.git#fix/electron-18",
    "eslint": "^9.11.0",
    "eslint-plugin-react": "^7.36.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.12",
    "globals": "^15.9.0",
    "mocha": "^10.0.0",
    "prettier": "^3.3.3",
    "spectron": "^19.0.0",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.6.0",
    "vite": "^5.4.7",
    "vite-tsconfig-paths": "^5.0.1"
  },
  "resolutions": {
    "@mui/styled-engine": "npm:@mui/styled-engine-sc@latest"
  }
}
